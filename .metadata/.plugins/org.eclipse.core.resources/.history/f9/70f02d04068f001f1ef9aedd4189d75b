package com.tap.launch;
import com.tap.classes.Bike;
import com.tap.classes.Car;
import com.tap.classes.Lorry;
import com.tap.factory.VehicleFactory;
import com.tap.interfaces.Vehicle;

public class Main {
	

	

	
	public static  void main(String args[]) {
		Vehicle vehicle1=VehicleFactory.CreateVehicle("Bike");
		vehicle1.drive();
		Vehicle vehicle2=VehicleFactory.CreateVehicle("Car");
		vehicle2.drive();
		Vehicle vehicle3=VehicleFactory.CreateVehicle("Lorry");
		vehicle3.drive();
	}
	}


